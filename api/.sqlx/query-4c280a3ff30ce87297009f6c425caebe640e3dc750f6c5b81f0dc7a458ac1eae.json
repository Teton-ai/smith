{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                d.id AS device_id,\n                d.serial_number,\n                d.release_id,\n                d.target_release_id,\n                d.last_ping,\n                dd.created_at AS added_at\n            FROM deployment_devices dd\n            JOIN device d ON dd.device_id = d.id\n            WHERE dd.deployment_id = $1\n            ORDER BY dd.created_at ASC\n            ",
