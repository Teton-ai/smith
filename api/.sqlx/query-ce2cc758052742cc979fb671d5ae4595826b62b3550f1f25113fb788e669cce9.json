{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                d.*,\n                COALESCE(ARRAY_AGG(l.name || '=' || dl.value), '{}') as \"labels!\"\n            FROM device d\n            LEFT JOIN device_label dl ON dl.device_id = d.id\n            LEFT JOIN label l ON l.id = dl.label_id\n            WHERE\n                token IS NOT NULL AND\n                token = $1\n            GROUP BY d.id\n            ",
